<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:right="-4dp"
        android:left="-4dp">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape
                android:shape="line">
                <stroke
                    android:color="@color/white"
                    android:width="6dp"/>
            </shape>
        </rotate>
    </item>
    <item
        android:right="-4dp"
        android:left="-4dp">
        <rotate
            android:fromDegrees="-45"
            android:toDegrees="-45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape
                android:shape="line">
                <stroke
                    android:color="@color/white"
                    android:width="6dp" />
            </shape>
        </rotate>
    </item>
</layer-list>
