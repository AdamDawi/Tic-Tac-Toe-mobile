<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape
                android:shape="line">
                <stroke
                    android:color="@color/white"
                    android:width="20dp"/>
                <corners android:radius="5dp"/>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:fromDegrees="-45"
            android:toDegrees="-45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape
                android:shape="line">
                <stroke
                    android:color="@color/white"
                    android:width="20dp" />
            </shape>
        </rotate>
    </item>
</layer-list>
